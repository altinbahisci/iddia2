<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
<div class="section nomargin nobg notopborder" style="padding: 80px 0 0 0;">
<div class="container ">
<div>
<div class="theTitle">
<h1>Üyelik Formu</h1>
</div>
<div class="js-form-controlers">
<div class="js-form-resultss"></div>

<div class="field">
 <%= f.email_field :email, autofocus: true, autocomplete: 'email' ,required:'required',class:'form-control input-lg',placeholder:'E-Posta Adresi (*)'%>
</div>
<div class="field">
 <%= f.password_field :password, autofocus: true, autocomplete: "off" ,required:'required',class:'form-control input-lg',placeholder:'Parola (*)'%>
</div>
<div class="field">
 <%= f.password_field :password_confirmation, autofocus: true, autocomplete: "off" ,required:'required',class:'form-control input-lg',placeholder:'Parola Onay (*)'%>
</div>
  <div class="actions">
    <%= f.submit "Üyelik Oluştur" ,class:"button regbtn"%>
  </div>

<br>
<br>
<br>
<br>
<br>
<br>
<br>
<br>
<br>
